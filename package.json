{
  "name": "ember-fusioncharts",
  "version": "2.1.0",
  "description": "A lightweight Ember component for FusionCharts JavaScript Charting Library",
  "keywords": [
    "ember-addon",
    "fusioncharts",
    "bindings",
    "wrapper",
    "fusioncharts-wrapper"
  ],
  "license": "MIT",
  "author": {
    "name": "FusionCharts",
    "email": "support@fusioncharts.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fusioncharts/ember-fusioncharts"
  },
  "bugs": {
    "url": "https://github.com/fusioncharts/ember-fusioncharts/issues"
  },
  "scripts": {
    "build": "ember build",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "cross-env COVERAGE=true ember test",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@embroider/test-setup": "^4.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-node-assets": "^0.2.2",
    "eslint": "^8.57.0",
    "handlebars": "^4.7.3",
    "jquery": "^3.5.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "mixin-deep": "^1.3.2",
    "websocket-extensions": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@ember/optional-features": "^2.1.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.3.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.7.4",
    "ember-cli": "~5.11.0",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~5.3.8",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.2.0",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.1.0",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.11.0",
    "ember-template-lint": "^5.13.0",
    "ember-welcome-page": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.12.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.1",
    "fusioncharts": "^3.13.3-sr.1",
    "loader.js": "^4.7.0",
    "prettier": "^3.3.3",
    "qunit": "^2.22.0",
    "qunit-dom": "^2.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "tracked-built-ins": "^3.3.0",
    "webpack": "^5.93.0"
  },
  "peerDependencies": {
    "ember-source": ">= 3.19.0 <= 5.11.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
